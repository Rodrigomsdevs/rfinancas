    <div class="mn-vh-100 d-flex align-items-center">
        <div class="container">
            <!-- Card -->
            <div class="card justify-content-center auth-card">
                <div class="row justify-content-center">
                    <div class="col-xl-10">
                        <h4 class="mb-5 font-20">RFINANÇAS</h4>

                        <form id="formRegister">
                            <div class="row">
                                <div class="col-lg-6">
                                    <!-- Form Group -->
                                    <div class="form-group mb-20">
                                        <label for="nome" class="mb-2 font-14 bold black">Nome</label>
                                        <input type="text" id="nome" class="theme-input-style" placeholder="Nome">
                                    </div>
                                    <!-- End Form Group -->
                                </div>

                                <div class="col-lg-6">
                                    <!-- Form Group -->
                                    <div class="form-group mb-20">
                                        <label for="sobrenome" class="mb-2 font-14 bold black">Sobrenome</label>
                                        <input type="text" id="sobrenome" class="theme-input-style" placeholder="Sobrenome">
                                    </div>
                                    <!-- End Form Group -->
                                </div>

                                <div class="col-lg-12">
                                    <!-- Form Group -->
                                    <div class="form-group mb-20">
                                        <label for="email" class="mb-2 font-14 bold black">Email</label>
                                        <input type="text" id="email" class="theme-input-style" placeholder="Email">
                                    </div>
                                    <!-- End Form Group -->
                                </div>


                                <div class="col-lg-6">
                                    <!-- Form Group -->
                                    <div class="form-group mb-20">
                                        <label for="senha" class="mb-2 font-14 bold black">Senha</label>
                                        <input type="password" id="senha" class="theme-input-style" placeholder="Password">
                                    </div>
                                    <!-- End Form Group -->
                                </div>

                                <div class="col-lg-6">
                                    <!-- Form Group -->
                                    <div class="form-group mb-20">
                                        <label for="r_senha" class="mb-2 font-14 bold black">Senha confirmação</label>
                                        <input type="password" id="r_senha" class="theme-input-style" placeholder="Password">
                                    </div>
                                    <!-- End Form Group -->
                                </div>
                            </div>

                            <div class="d-flex align-items-center pt-4">
                                <button type="submit" class="btn long mr-20">Registrar</button>
                                <span class="font-12 d-block"><a href="/user/login" class="bold">Entre</a>, caso você já tenha uma conta.</span>
                            </div>
                        </form>
                    </div>
